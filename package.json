{
  "name": "genshin-party",
  "version": "0.0.3",
  "keywords": [
    "cli",
    "genshin",
    "genshin-impact"
  ],
  "repository": {
    "url": "git+https://github.com/andrewtran1995/genshin-party.git"
  },
  "license": "MIT",
  "type": "module",
  "bin": {
    "genshin-party": "dist/index.js"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsc",
    "build:tsup": "tsup --clean --format cjs,esm src/index.ts --dts",
    "dev": "tsx src/index.ts",
    "postinstall": "husky",
    "lint": "ts-standard src",
    "lint:fix": "ts-standard --fix src",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prepare": "husky",
    "sort-package-json": "sort-package-json",
    "test": "ava"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "build/"
      },
      "compile": "tsc"
    }
  },
  "dependencies": {
    "@commander-js/extra-typings": "^12.0.0",
    "@inquirer/select": "^2.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "genshin-db": "^5.1.1",
    "lodash": "^4.17.21",
    "ts-pattern": "^5.0.8"
  },
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "@types/lodash": "^4.14.202",
    "ava": "^6.1.1",
    "husky": "^9.0.11",
    "pinst": "^3.0.0",
    "sort-package-json": "^2.7.0",
    "ts-standard": "^12.0.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "type-fest": "^4.10.2",
    "typescript": "^5.3.3"
  },
  "packageManager": "yarn@4.1.0"
}
