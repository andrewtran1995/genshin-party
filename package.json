{
	"name": "genshin-party",
	"version": "0.3.1",
	"keywords": [
		"cli",
		"genshin",
		"genshin-impact"
	],
	"repository": {
		"url": "git+https://github.com/andrewtran1995/genshin-party.git"
	},
	"license": "MIT",
	"type": "module",
	"exports": {
		"import": "./dist/index.js"
	},
	"bin": "dist/bin.js",
	"files": [
		"dist/*"
	],
	"scripts": {
		"build": "tsc",
		"dev": "tsx src/bin.ts",
		"postinstall": "husky",
		"lint": "biome check --error-on-warnings",
		"lint:fix": "yarn lint --write",
		"prepack": "pinst --disable",
		"postpack": "pinst --enable",
		"pre-commit": "tsx src/pre-commit.ts",
		"prepare": "husky",
		"sort-package-json": "sort-package-json",
		"test": "vitest --run --typecheck",
		"test:watch": "vitest --typecheck"
	},
	"dependencies": {
		"@commander-js/extra-typings": "^14.0.0",
		"@inquirer/select": "^4.2.3",
		"ansis": "^4.1.0",
		"arktype": "^2.1.20",
		"commander": "^14.0.0",
		"dotenv": "^16.5.0",
		"genshin-db": "^5.2.1",
		"remeda": "^2.23.0",
		"ts-pattern": "^5.7.1",
		"xstate": "^5.19.4"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^24.0.2",
		"execa": "^9.6.0",
		"husky": "^9.1.7",
		"listr2": "^8.3.3",
		"pinst": "^3.0.0",
		"sort-package-json": "^3.2.1",
		"tsx": "^4.19.4",
		"type-fest": "^4.41.0",
		"typescript": "^5.8.3",
		"vitest": "^3.2.2"
	},
	"packageManager": "yarn@4.9.1"
}
