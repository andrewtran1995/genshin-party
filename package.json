{
  "name": "genshin-party",
  "version": "0.0.13",
  "keywords": [
    "cli",
    "genshin",
    "genshin-impact"
  ],
  "repository": {
    "url": "git+https://github.com/andrewtran1995/genshin-party.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "import": "./dist/index.js"
  },
  "bin": {
    "genshin-party": "dist/bin.js"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/bin.ts",
    "postinstall": "husky",
    "lint": "xo src",
    "lint:fix": "xo --fix src",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prepare": "husky",
    "sort-package-json": "sort-package-json",
    "test": "vitest --run --typecheck"
  },
  "xo": {
    "semicolon": false
  },
  "dependencies": {
    "@commander-js/extra-typings": "^12.0.1",
    "@inquirer/select": "^2.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "genshin-db": "^5.1.1",
    "lodash": "^4.17.21",
    "ts-pattern": "^5.0.8"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/xo": "^0",
    "execa": "^8.0.1",
    "husky": "^9.0.11",
    "pinst": "^3.0.0",
    "sort-package-json": "^2.8.0",
    "tsx": "^4.7.1",
    "type-fest": "^4.12.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1",
    "xo": "^0.58.0"
  },
  "packageManager": "yarn@4.1.0"
}
